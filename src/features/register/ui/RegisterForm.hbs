<form class="registration-form" action="#">
  <div class="registration-form__inputs">
    {{>Input label="First name" id="first_name" name="first_name" type="text" autocomplete="given-name"}}
    {{>Input label="Last name" id="second_name" name="second_name" type="text" autocomplete="family-name"}}
    {{>Input label="Login" id="login" name="login" type="text" autocomplete="username"}}
    {{>Input label="Email" id="email" name="email" type="text" autocomplete="email"}}
    {{>Input label="Phone" id="phone" name="phone" type="tel" autocomplete="tel"}}
    {{>Input label="Password" id="password" name="password" type="password" autocomplete="new-password"}}
  </div>
  <div class="registration-form__buttons">
    {{>Button label="Sign up" type="submit"}}
    {{>Link text="Sign in" href="/sign-in"}}
  </div>
</form>
